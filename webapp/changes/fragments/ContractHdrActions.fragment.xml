<!-- Use stable and unique id's!-->
<core:FragmentDefinition xmlns:core='sap.ui.core' xmlns="sap.m">
	
	<Button id="idCtrHeadManageQuotaBtn" text="{i18n>ManageQuotaButtonLabel}"
		press=".extension.vwks.nlp.s2p.mm.pctrcentral.manage.ObjectPageExt.onManageQuotaButtonPress"
		tooltip="{i18n>ManageQuotaButtonLabel}"/>
    <Button 
		id="idCtrPFORelatedLinkBtn"
		text="{i18n>PFORelatedLink}" 
		icon="sap-icon://process"
		press=".extension.vwks.nlp.s2p.mm.pctrcentral.manage.ObjectPageExt.onPFOLinkPress"
		tooltip="{i18n>PFORelatedLink}">
	</Button>
	<Button id="idCtrHeadBrnadInfoCopyBtn" 
		text="{i18n>CopyInfoContractButton}"
		tooltip="{i18n>CopyInfoContractButton}"
		press=".extension.vwks.nlp.s2p.mm.pctrcentral.manage.ObjectPageExt.onBrandInfoCopyBtnPress" 
		enabled="{InfoContractFlag}"
		visible="{= !${ui>/editable}}"/>
</core:FragmentDefinition>
