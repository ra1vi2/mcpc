<!-- Use stable and unique id's!-->
<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Column id="idDocHistoryCol">
		<Text id="idDocumentHistoryColText" text="{i18n>DocumentHistoryLabel}" tooltip="{i18n>DocumentHistoryTooltip}"/>
		<customData>
			<core:CustomData id="idDocHistCustomData" key="p13nData"
				value='\{"columnKey": "DocumentStatusName", "leadingProperty": "DocumentStatusName","columnIndex" : "3"}'/>
		</customData>
	</Column>
	<!--Supplier Overall Status-->
	<!-- Removed Sort and Filter properties as part of INC 342644 fix -->
	<Column id="idCtrOverallStatusColumn">
		<Text id="idCtrOverallStatusColumnText" text="{i18n>SupplierOverallStatusLabel}"/>
		<customData>
			<core:CustomData id="idCtrOverallStatusCustomData" key="p13nData"
				value='\{ "columnKey": "SupplierOverallStatus", "leadingProperty":"SupplierOverallStatus", "columnIndex" : "2"}'/>
		</customData>
	</Column>
	<!--Pricing Conditions-->
	<Column id="idPricingConditionsIconColumn">
		<Text id="idPricingConditionsIconColumnText"
			text="{i18n|sap.suite.ui.generic.template.ListReport|C_CentralPurchaseContractTP>PricingConditionsText}"/>
		<customData>
			<core:CustomData key="p13nData" value='\{"columnKey" : "pricingConditionsIconColumn", "columnIndex" : "13"}' id="idCustomData"/>
		</customData>
	</Column>
    <!--Jump to Distribution Line-->
    <Column id="idDistributionLineIconColumn">
        <Text id="idDistributionLineIconColumnText" text="{i18n|sap.suite.ui.generic.template.ListReport|C_CentralPurchaseContractTP>JumpToDistributionColumnLabel}"/>
        <customData>
            <core:CustomData id="idDistributionLineCustomData" key="p13nData" value='\{"columnKey" :"distributionLineIconColumn", "columnIndex" : "14"}' />
        </customData>
    </Column>
    <!--Jump to Quota Details-->
    <Column id="idListReportQuotaColumn">
		<Text id="idListReportQuotaColumnText" text="{i18n>QuotaDetails}"/>
		<customData>
			<core:CustomData id="idListReportQuotaCustomData" key="p13nData" value='\{ "columnKey": "Quota", "leadingProperty":"Quota", "columnIndex" : "15" }'/>
        </customData>
	</Column>
</core:FragmentDefinition>